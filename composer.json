    "type": "yii2-extension",
    "keywords": ["yii2","extension","yii2-async","async","php-async"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": ">7.1"
    },
    "require-dev": {
        "symfony/stopwatch": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "vxm\\async\\": "/src"
        }
    },
    "repositories": [
        {
            "type": "composer",
